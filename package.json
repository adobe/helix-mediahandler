{
  "name": "@adobe/helix-mediahandler",
  "version": "2.5.20",
  "description": "Helix Mediahandler Library",
  "main": "src/index.js",
  "type": "module",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "c8 mocha --spec=test/*.test.js --spec=test/mp4/*.test.js",
    "lint": "eslint .",
    "semantic-release-dry": "semantic-release --dry-run --branches $CI_BRANCH",
    "semantic-release": "semantic-release",
    "docs": "npx typedoc --options .typedoc.cjs",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-mediahandler"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-mediahandler/issues"
  },
  "homepage": "https://github.com/adobe/helix-mediahandler#readme",
  "dependencies": {
    "@adobe/fetch": "4.1.8",
    "@aws-sdk/client-s3": "3.651.1",
    "@aws-sdk/lib-storage": "3.651.1",
    "fetch-retry": "6.0.0",
    "image-size": "1.1.1",
    "mime": "4.0.4"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "2.0.7",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/node": "20.16.5",
    "c8": "10.1.2",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "fs-extra": "11.2.0",
    "husky": "9.1.6",
    "junit-report-builder": "5.0.0",
    "lint-staged": "15.2.10",
    "mocha": "10.7.3",
    "mocha-multi-reporters": "1.5.1",
    "mocha-suppress-logs": "0.5.1",
    "nock": "13.5.5",
    "semantic-release": "24.1.1",
    "typedoc": "0.26.7",
    "typedoc-plugin-markdown": "4.2.7",
    "typescript": "4.8.3",
    "xml2js": "0.6.2"
  },
  "mocha": {
    "require": [
      "test/setup-env.js",
      "mocha-suppress-logs"
    ],
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
